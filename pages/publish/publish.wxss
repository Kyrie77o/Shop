page{
  background: #eee;
}
.row{
  overflow:hidden;
  line-height: 100rpx;
  border-bottom: 1px solid #ccc;
  background: #fff;
  color:#777;
}
.submit-button{
  margin:10px 8px 0 8px;
  background: #ff9700;
  line-height: 38px;
  border-radius: 6px;
  text-align: center;
  color: #fff;

}
.title{
  padding-left:10px;
  width: 100px;
  float: left;
}
.info{
  line-height: 100rpx;
}
.input-info{
  height: 100rpx;
  line-height:100rpx ;
}
.input-inf{
  line-height: 50px;
  color:#777;
  text-align: center;
  font-size: 30rpx;
}
.success{
  background: #fff;
  padding: 20px;
}
.congratulations{
  text-align: center;
  line-height: 100px;
  font-size: 16px;
}
.success-icon{
  position: relative;
  top:6px;
  margin-right: 10px;
}
.back-button{
  margin:0 10px

}
.header {
  position: relative;
  display: flex;
  line-height: 250rpx;
  height:250rpx;
  color: #777;
  background: #fff;
}
.header .title{
  vertical-align: middle;
}
.avatar{
  font-size: 20rpx;
}
.avatar image {
  width: 200rpx;
  height: 220rpx;
  background: #f2f2f2;
  vertical-align: middle;
}


